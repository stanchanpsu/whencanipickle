{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
      "strictNullChecks": true,
      "allowJs": true,
      "baseUrl": ".",
      "paths": {
        "@assets/*": [
          "src/assets/*"
        ],
        "@components/*": [
          "src/components/*"
        ],
        "@content/*": [
          "src/content/*"
        ],
        "@layouts/*": [
          "src/layouts/*"
        ],
        "@pages/*": [
          "src/pages/*"
        ]
      }
    }
  }
